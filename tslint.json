{
    "extends": "@blueprintjs/tslint-config",
    "no-implicit-dependencies": false,
    "no-unused-expression": false,
    "rules": {
        "jsx-no-lambda": false,
        "member-access": [false, "no-public"],
        "max-classes-per-file": false,
        "no-console": false,
        "indent": [true, "tabs", 2]
    }
}
